{
  "name": "@treacherous/core",
  "version": "0.17.9",
  "license": "MIT",
  "dependencies": {
    "property-resolver": "^0.0.11",
    "event-js": "^0.1.3",
    "tslib": "^1.9.2"
  },
  "main": "dist/commonjs/index.js",
  "module": "dist/es2015/index.js",
  "types": "dist/definitions/index.d.ts",
  "bugs": {
    "url": "https://github.com/grofit/treacherous/issues"
  },
  "files": [
    "dist/**/*",
    "index.d.ts",
    "readme.md"
  ],
  "keywords": [
    "validation",
    "validate"
  ],
  "scripts": {
    "build": "gulp compile",
    "test": "mocha --exit -r ts-node/register tests/specs/**/*.ts",
    "test-with-coverage": "nyc mocha --exit -r ts-node/register -r source-map-support/register --recursive tests/specs/**/*.ts",
    "test-with-codacy": "nyc mocha --report lcovonly -R spec --exit -r ts-node/register -r source-map-support/register --recursive tests/specs/**/*.ts && cat ./coverage/lcov.info | codacy-coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grofit/treacherous"
  },
  "devDependencies": {
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-es6-exporter": "^0.0.5",
    "gulp-rename": "^1.3.0",
    "gulp-typedoc": "^2.2.0",
    "gulp-typescript": "^4.0.2",
    "gulp-uglify": "^3.0.0",
    "merge2": "^1.2.2",
    "run-sequence": "^2.2.1",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.4.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "ts-node": "^6.1.1",
    "webpack-stream": "^4.0.3",
    "@types/bluebird": "^3.5.20",
    "@types/chai": "^4.1.4",
    "@types/chai-spies": "^0.0.1",
    "@types/mocha": "~5.2.2",
    "nyc": "^14.0.0",
    "source-map-support": "^0.5.12",
    "codacy-coverage": "^3.4.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/locales/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
